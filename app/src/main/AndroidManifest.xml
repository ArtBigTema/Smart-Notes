<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="av.smartnotes">

    <meta-data
        android:name="DB_NODE_NAME"
        android:value="Node" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".AppSmartNodes_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailActivity_" />
        <activity android:name=".activity.EditDetailActivity_" />
        <activity android:name=".activity.activity_with.ActivityWithToolbar_" />
        <activity android:name=".activity.activity_with.ActivityWithFabMenu_" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".activity.MapsActivity_" />
    </application>

</manifest>